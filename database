CREATE TABLE ezen_member(
id varchar2(15) NOT NULL,
pwd varchar2(15) NOT NULL,
name varchar2(20) NOT NULL,
phone varchar2(30),
email varchar2(30),
gender number(1) ,
age number(3) NOT null,
admin number(1),
enter DATE DEFAULT sysdate,
PRIMARY key(id)
);


